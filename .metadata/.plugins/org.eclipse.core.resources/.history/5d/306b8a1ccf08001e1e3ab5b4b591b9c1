package com.coding404.util;

public class UserAuthFilter {
	
	
	
	
	
	//세션을 얻음
	Http
	//회원이 로그인 할 때 저장한 인증값
	String user_id = (String)session.get
	
	//로그인이 안 됨
	if(user_id == null) {
	
		String path = req.getContetPath() + "/user/user_"
	
	
	res.setContentType();
	PrintWriter out = res.get
	
	out.println("<script>");
	out.println("alert('로그인이 필요한 서비스입니다');");
	out.println("location.href='" + path + ""';");
	out.println("</script>");
	
	
